@model MVC_Model.Models.Student

@{
    ViewBag.Title = "View Model (Raw)";
}

<h2>@ViewBag.Title</h2>
<br>
<h3>@ViewBag.Message</h3>
<br>
<h4>@ViewBag.Message2</h4>
<br>
<p>@Html.Label("nameLabel", "Name"):@Html.Raw("&nbsp;")@Model.FirstName</p>
<p>@Html.Label("middleNameLabel", "Middle Name"):@Html.Raw("&nbsp;")@Model.MiddleName</p>
<p>@Html.Label("lastNameLabel", "Lastname"):@Html.Raw("&nbsp;")@Model.LastName</p>
<p>@Html.Label("emailLabel", "Email"):@Html.Raw("&nbsp;")@Model.Email</p>
<p>@Html.Label("birthDayLabel", "Birthday"):@Html.Raw("&nbsp;")@Model.Birthday</p>
